                            <span class="pull-right-container">
                                <i class="fa fa-angle-down pull-right"></i>
                            </span>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="@Url.Action("Index","Project")">Project Management</a></li>
                            <li><a href="@Url.Action("Index","Personnel")">Personnel Management</a></li>
                            <li><a href="#">Data Uploading</a></li>
                        </ul>
                    </li>
                </ul>
                <!-- /.sidebar-menu -->
            </section>
            <!-- /.sidebar -->
        </aside>
