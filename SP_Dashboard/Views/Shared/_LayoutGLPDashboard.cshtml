<body class="hold-transition skin sidebar-mini">
    <div class="wrapper">
        <!-- Main Header -->
        <header class="main-header">
            <!-- Logo -->
            <a href="/" class="logo">
                <!-- mini logo for sidebar mini 50x50 pixels -->
                <span class="logo-mini"><b>SP</b></span>
                <!-- logo for regular state and mobile devices -->
                <span class="logo-lg"><b>SP</b>DashBoard</span>
            </a>
            <!-- Header Navbar -->
            <nav class="navbar navbar-static-top" role="navigation">
                <!-- Sidebar toggle button-->
                <a href="#" class="sidebar-toggle" data-toggle="push-menu" role="button">
                    <span class="sr-only">Toggle navigation</span>
                </a>
                <div class="navbar-custom-menu">
                    <ul class="nav navbar-nav">
                        <li class="user user-menu dropdown-custom">
                            @Html.Partial("_LoginPartial")
                        </li>
                        <!-- Control Sidebar Toggle Button -->
                        @if (UserManager.Authenticated)
                        {
                            <li>
                                <a href="#" data-toggle="control-sidebar"><i class="fa fa-cogs"></i></a>
                            </li>
                        }

                    </ul>
                </div>
            </nav>
        </header>
        <!-- Left side column. contains the logo and sidebar -->
        <aside class="main-sidebar">
            <!-- sidebar: style can be found in sidebar.less -->
            <section class="sidebar">
                <!-- Sidebar Menu -->
                <ul class="sidebar-menu" data-widget="tree">
                    <li class="header">MENU</li>
                    <!-- Optionally, you can add icons to the links -->
                    <li class="treeview">
                        <a href="/Dashboard/">
                            <i class="fa fa-dashboard"></i> <span>Dashboard</span>
                            <span class="pull-right-container">
                                <i class="fa fa-angle-down pull-right"></i>
                            </span>
                        </a>
                        @*@Html.Partial("_DashBoardPartial")*@
                        <ul class="treeview-menu">
                            <li><a href="/Dashboard/ConsultancyCenter/">Consultancy Center</a></li>
                            <li><a href="/Dashboard/Internship/">Internship</a></li>
                            <li><a href="/Dashboard/ConsultancyCenter/">Overseas Intersnhip</a></li>
                            <li><a href="/Dashboard/GLP/">Global Learning Programe</a></li>
                            <li><a href="/Dashboard/Entrepreuneurship/">Entrepreuneurship</a></li>

                            @*<li><a href="#">Role Management</a></li>*@
