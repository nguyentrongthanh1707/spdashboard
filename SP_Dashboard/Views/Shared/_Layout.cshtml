    <div class="wrapper">
        <!-- Main Header -->
        <header class="main-header">
            <!-- Logo -->
            <a href="/" class="logo">
                <!-- mini logo for sidebar mini 50x50 pixels -->
                <span class="logo-mini"><b>SP</b></span>
                <!-- logo for regular state and mobile devices -->
                <span class="logo-lg"><b>SP</b>DashBoard</span>
            </a>
            <!-- Header Navbar -->
            <nav class="navbar navbar-static-top">
                <!-- Sidebar toggle button-->
                <a href="#" class="sidebar-toggle" data-toggle="push-menu" role="button">
                    <span class="sr-only">Toggle navigation</span>
                </a>
                <div class="navbar-custom-menu">
                    <ul class="nav navbar-nav">
                        @*<li class="user user-menu dropdown-custom">

                            </li>*@
                        <li class="dropdown user user-menu">
                            @Html.Partial("_LoginPartial")
                        </li>
                        @if ((routeData.Values["controller"] + "").ToLower() != "dashboard")
                        {
                            <li>
                                <a href="#" data-toggle="control-sidebar"><i class="fa fa-gears"></i></a>
                            </li>
                        }
                    </ul>
                </div>
            </nav>
        </header>
        <!-- Left side column. contains the logo and sidebar -->
        <aside class="main-sidebar">
            <!-- sidebar: style can be found in sidebar.less -->
            <section class="sidebar">
                <ul class="sidebar-menu" data-widget="tree">
                    <li class="header">MENU</li>
                    <li class="@((routeData.Values["controller"] + "").ToLower() == "dashboard" ? "active " : "")treeview">
                        <a href="#">
                            <i class="fa fa-dashboard"></i> <span>Dashboard</span>
                            <span class="pull-right-container">
                                <i class="fa fa-angle-left pull-right"></i>
                            </span>
                        </a>
                        @Html.Partial("_DashBoardPartial")
                    </li>
                    <li class="@((routeData.Values["controller"] + "").ToLower() == "account" ? "active " : "")treeview">
                        <a href="#">
                            <i class="fa fa-user"></i>
                            <span>Account</span>
                            <span class="pull-right-container">
                                <i class="fa fa-angle-left pull-right"></i>
                            </span>
                        </a>
                        <ul class="treeview-menu">
                            <li class="@((routeData.Values["controller"] + "").ToLower() == "account" ? "active" : "")"><a href="/Account"><i class="fa fa-circle-o"></i> Account Management</a></li>
                        </ul>
                    </li>
                    <li class="@(((routeData.Values["controller"] + "").ToLower() == "project" || (routeData.Values["controller"] + "").ToLower() == "personnel" || (routeData.Values["controller"] + "").ToLower() == "dataupload") ? "active " : "")treeview">
                        <a href="#">
                            <i class="fa fa-list"></i>
                            <span>Management</span>
                            <span class="pull-right-container">
                                <i class="fa fa-angle-left pull-right"></i>
                            </span>
                        </a>
                        <ul class="treeview-menu">
                            <li class="@((routeData.Values["controller"] + "").ToLower() == "project" ? "active" : "")"><a href="@Url.Action("Index","Project")"><i class="fa fa-circle-o"></i> Project Management</a></li>
                            <li class="@((routeData.Values["controller"] + "").ToLower() == "personnel" ? "active" : "")"><a href="@Url.Action("Index","Personnel")"><i class="fa fa-circle-o"></i> Personnel Management</a></li>
                            <li class="@((routeData.Values["controller"] + "").ToLower() == "dataupload" ? "active" : "")"><a href="#"><i class="fa fa-circle-o"></i> Data Uploading</a></li>
                        </ul>
                    </li>
                </ul>
                <!-- /.sidebar-menu -->
            </section>
            <!-- /.sidebar -->
        </aside>
